#!/bin/bash

# 编译并拷贝到个人目录
DIR=~/
cd $DIR/yxs/THKServer/Game; pwd && make -j16 -f Makefile_Debug
cp $DIR/yxs/THKServer/Bin/Debug/game.so $DIR/yxs/AutTest/Logic
cp $DIR/yxs/THKServer/Bin/Debug/game.so $DIR/yxs/AutTest/Ctrl
